@attribute [Microsoft.AspNetCore.Components.RouteAttribute(C.Routes.Analysis)]

<div class="container text-center">
    <div class="row">
        <div class="col">
            <canvas id="month-count"></canvas>
        </div>
        <div class="col">
            <canvas id="year-count"></canvas>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <canvas id="month-price"></canvas>
        </div>
        <div class="col">
            <canvas id="year-price"></canvas>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <canvas id="month-removed"></canvas>
        </div>
        <div class="col">
            <canvas id="year-removed"></canvas>
        </div>
    </div>
</div>